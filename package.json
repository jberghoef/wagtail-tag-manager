{
  "name": "wagtail-tag-manager",
  "version": "0.22.4",
  "description": "A Wagtail add-on for managing tags.",
  "main": "webpack.config.js",
  "browserslist": "> 0.25%, not dead",
  "scripts": {
    "build": "webpack --mode production",
    "dev": "webpack --mode development",
    "watch": "webpack --mode development --watch",
    "cypress:open": "CYPRESS_CRASH_REPORTS=0 cypress open",
    "cypress:run": "CYPRESS_CRASH_REPORTS=0 wait-on http://127.0.0.1:8000/ && cypress run",
    "format": "prettier --write './frontend/**/*'"
  },
  "repository": "github.com/jberghoef/wagtail-tag-manager",
  "author": "Jasper Berghoef",
  "license": "BSD-3-Clause",
  "devDependencies": {
    "@babel/core": "^7.11.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.11.0",
    "@babel/polyfill": "^7.10.4",
    "@babel/preset-env": "^7.11.0",
    "@types/codemirror": "^0.0.97",
    "@types/js-cookie": "^2.2.6",
    "babel-loader": "^8.0.6",
    "css-loader": "^4.2.1",
    "cypress": "^4.12.1",
    "mini-css-extract-plugin": "^0.10.0",
    "node-sass": "^4.14.1",
    "prettier": "^2.0.5",
    "sass-loader": "^9.0.3",
    "serialize-javascript": "^4.0.0",
    "style-loader": "^1.2.1",
    "ts-loader": "^8.0.2",
    "typescript": "^3.9.7",
    "wait-on": "^5.2.0",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12"
  },
  "dependencies": {
    "codemirror": "^5.56.0",
    "js-cookie": "^2.2.1",
    "whatwg-fetch": "^3.4.0"
  }
}
